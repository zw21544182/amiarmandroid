<?xml version="1.0" encoding="utf-8"?>
<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/nodelayout"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_centerHorizontal="true"
    android:layout_centerVertical="true"
    android:background="#000"
    android:columnCount="3"
    android:padding="1dp"
    android:rowCount="10">

    <TextView
        style="@style/GrideItme"
        android:layout_width="200dp"
        android:layout_height="26dp"
        android:layout_columnSpan="1"
        android:layout_rowSpan="1"
        android:text="漂浮物检出次数" />

    <TextView
        android:id="@+id/tvPiaoFuNum"

        style="@style/GrideItme"
        android:layout_width="50dp"
        android:layout_height="26dp"
        android:layout_columnSpan="1"

        android:layout_rowSpan="1"
        android:text="0" />

    <TextView
        android:id="@+id/tvPiaoFuRes"

        style="@style/GrideItme"
        android:layout_width="50dp"
        android:layout_columnSpan="1"
        android:layout_gravity="fill"
        android:layout_margin="1dp"
        android:layout_rowSpan="1"
        android:text="阴性" />

    <TextView
        style="@style/GrideItme"
        android:layout_width="200dp"
        android:layout_height="25dp"
        android:layout_columnSpan="1"
        android:layout_rowSpan="1"
        android:text="速降物检出率(%)" />

    <TextView
        android:id="@+id/tvSuJianPer"
        style="@style/GrideItme"
        android:layout_width="50dp"
        android:layout_height="25dp"
        android:layout_columnSpan="1"

        android:layout_rowSpan="1"
        android:text="0" />


    <TextView
        android:id="@+id/tvSuJianRes"
        style="@style/GrideItme"
        android:layout_width="50dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_columnSpan="1"
        android:layout_gravity="fill"
        android:layout_rowSpan="2"
        android:text="阴性" />


    <TextView
        style="@style/GrideItme"
        android:layout_width="200dp"
        android:layout_height="25dp"
        android:layout_columnSpan="1"
        android:layout_rowSpan="1"
        android:text="速降物时间比例(%)" />

    <TextView
        android:id="@+id/tvsuJianTime"
        style="@style/GrideItme"
        android:layout_width="50dp"
        android:layout_height="25dp"
        android:layout_columnSpan="1"

        android:layout_rowSpan="1"
        android:text="0" />

    <TextView
        style="@style/GrideItme"
        android:layout_width="200dp"
        android:layout_height="25dp"
        android:layout_columnSpan="1"
        android:layout_rowSpan="1"
        android:text="40-50um异物检出率(%)" />

    <TextView
        android:id="@+id/tvCheck40"
        style="@style/GrideItme"
        android:layout_width="50dp"
        android:layout_height="25dp"
        android:layout_columnSpan="1"
        android:layout_rowSpan="1"
        android:text="0" />

    <TextView
        android:id="@+id/tvCheckRes"
        style="@style/GrideItme"
        android:layout_width="50dp"
        android:layout_columnSpan="1"
        android:layout_gravity="fill"
        android:layout_margin="1dp"
        android:layout_rowSpan="4"
        android:text="阴性" />

    <TextView
        style="@style/GrideItme"
        android:layout_width="200dp"
        android:layout_height="25dp"
        android:layout_columnSpan="1"
        android:layout_rowSpan="1"
        android:text="50-60um异物检出率(%)" />

    <TextView
        android:id="@+id/tvCheck50"
        style="@style/GrideItme"
        android:layout_width="50dp"
        android:layout_height="25dp"
        android:layout_columnSpan="1"
        android:layout_rowSpan="1"
        android:text="0" />

    <TextView
        style="@style/GrideItme"
        android:layout_width="200dp"
        android:layout_height="25dp"
        android:layout_columnSpan="1"
        android:layout_rowSpan="1"
        android:text="60-70um异物检出率(%)" />

    <TextView
        android:id="@+id/tvCheck60"
        style="@style/GrideItme"
        android:layout_width="50dp"
        android:layout_height="25dp"
        android:layout_columnSpan="1"
        android:layout_rowSpan="1"
        android:text="0" />

    <TextView
        style="@style/GrideItme"
        android:layout_width="200dp"
        android:layout_height="25dp"
        android:layout_columnSpan="1"
        android:layout_rowSpan="1"
        android:text="70um以上异物检出率(%)" />

    <TextView
        android:id="@+id/tvCheck70"
        style="@style/GrideItme"
        android:layout_width="50dp"
        android:layout_height="25dp"
        android:layout_columnSpan="1"
        android:layout_rowSpan="1"
        android:text="0" />

    <TextView
        style="@style/GrideItme"
        android:layout_width="200dp"
        android:layout_height="25dp"
        android:layout_columnSpan="1"
        android:layout_rowSpan="1"
        android:text="40-50um异物检出数(颗)" />

    <TextView
        android:id="@+id/tvCheckNum40"
        style="@style/GrideItme"
        android:layout_width="50dp"
        android:layout_height="25dp"
        android:layout_columnSpan="1"
        android:layout_rowSpan="1"
        android:text="0" />

    <TextView
        android:id="@+id/tvMinNumRes"
        style="@style/GrideItme"
        android:layout_width="50dp"
        android:layout_columnSpan="1"
        android:layout_gravity="fill"
        android:layout_margin="1dp"
        android:layout_rowSpan="2"
        android:text="阴性" />

    <TextView
        style="@style/GrideItme"
        android:layout_width="200dp"
        android:layout_height="25dp"
        android:layout_columnSpan="1"
        android:layout_rowSpan="1"
        android:text="50-70um" />

    <TextView
        android:id="@+id/tvCheckNum50"
        style="@style/GrideItme"
        android:layout_width="50dp"
        android:layout_height="25dp"
        android:layout_columnSpan="1"
        android:layout_rowSpan="1"
        android:text="0" />

    <TextView
        style="@style/GrideItme"
        android:layout_width="200dp"
        android:layout_columnSpan="1"
        android:layout_gravity="fill"
        android:layout_rowSpan="1"
        android:text="70um以上异物检出数(颗)X" />

    <TextView
        android:id="@+id/tvCheckNum70"
        style="@style/GrideItme"
        android:layout_width="50dp"
        android:layout_height="25dp"
        android:layout_columnSpan="1"
        android:layout_rowSpan="1"
        android:text="0" />

    <TextView
        android:id="@+id/tvMaxCheckRes"
        style="@style/GrideItme"
        android:layout_width="50dp"
        android:layout_columnSpan="1"
        android:layout_gravity="fill"
        android:layout_margin="1dp"
        android:layout_rowSpan="2"
        android:text="阴性" />
</GridLayout>