<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/nodelayout"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_centerHorizontal="true"
    android:layout_centerVertical="true"
    android:layout_gravity="center"
  >
    <LinearLayout
        android:layout_width="304dp"
        android:layout_height="262dp"
        android:background="@color/colorBlack"
        android:orientation="horizontal"
        android:padding="1dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">

        <LinearLayout
            android:layout_width="251dp"
            android:layout_height="263dp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="251dp"
                android:layout_height="26dp"
                android:orientation="horizontal">


                <TextView
                    style="@style/GrideItme"
                    android:layout_width="200dp"
                    android:layout_height="26dp"
                    android:text="漂浮物检出次数" />

                <TextView
                    android:id="@+id/tvPiaoFuNum"
                    style="@style/GrideItme"
                    android:layout_width="50dp"
                    android:layout_height="26dp"
                    android:layout_columnSpan="1"
                    android:layout_marginLeft="1dp"
                    android:text="0" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="251dp"
                android:layout_height="26dp"
                android:layout_marginTop="1dp"
                android:orientation="horizontal">

                <TextView
                    style="@style/GrideItme"
                    android:layout_width="200dp"
                    android:layout_height="25dp"
                    android:text="速降物检出率(%)" />

                <TextView
                    android:id="@+id/tvSuJianPer"
                    style="@style/GrideItme"
                    android:layout_width="50dp"
                    android:layout_height="25dp"
                    android:layout_marginLeft="1dp"
                    android:text="0" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="251dp"
                android:layout_height="26dp"
                android:orientation="horizontal">

                <TextView
                    style="@style/GrideItme"
                    android:layout_width="200dp"
                    android:layout_height="25dp"
                    android:text="速降物时间比例(%)" />

                <TextView
                    android:id="@+id/tvsuJianTime"

                    style="@style/GrideItme"
                    android:layout_width="50dp"
                    android:layout_height="25dp"
                    android:layout_marginLeft="1dp"
                    android:text="0" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="251dp"
                android:layout_height="26dp"
                android:orientation="horizontal">

                <TextView
                    style="@style/GrideItme"
                    android:layout_width="200dp"
                    android:layout_height="25dp"
                    android:text="40-50um异物检出率(%)" />

                <TextView
                    android:id="@+id/tvCheck40"
                    style="@style/GrideItme"
                    android:layout_width="50dp"
                    android:layout_height="25dp"
                    android:layout_marginLeft="1dp"
                    android:text="0" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="251dp"
                android:layout_height="26dp"
                android:orientation="horizontal">

                <TextView
                    style="@style/GrideItme"
                    android:layout_width="200dp"
                    android:layout_height="25dp"
                    android:text="50-60um异物检出率(%)" />

                <TextView
                    android:id="@+id/tvCheck50"

                    style="@style/GrideItme"
                    android:layout_width="50dp"
                    android:layout_height="25dp"
                    android:layout_marginLeft="1dp"
                    android:text="0" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="251dp"
                android:layout_height="26dp"
                android:orientation="horizontal">

                <TextView
                    style="@style/GrideItme"
                    android:layout_width="200dp"
                    android:layout_height="25dp"
                    android:text="60-70um异物检出率(%)" />

                <TextView
                    android:id="@+id/tvCheck60"

                    style="@style/GrideItme"
                    android:layout_width="50dp"
                    android:layout_height="25dp"
                    android:layout_marginLeft="1dp"
                    android:text="0" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="251dp"
                android:layout_height="26dp"
                android:orientation="horizontal">

                <TextView
                    style="@style/GrideItme"
                    android:layout_width="200dp"
                    android:layout_height="25dp"
                    android:text="70um以上异物检出率(%)" />

                <TextView
                    android:id="@+id/tvCheck70"

                    style="@style/GrideItme"
                    android:layout_width="50dp"
                    android:layout_height="25dp"
                    android:layout_marginLeft="1dp"
                    android:text="0" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="251dp"
                android:layout_height="26dp"
                android:orientation="horizontal">

                <TextView
                    style="@style/GrideItme"
                    android:layout_width="200dp"
                    android:layout_height="25dp"
                    android:text="40-50um异物检出数(颗)" />

                <TextView
                    android:id="@+id/tvCheckNum40"

                    style="@style/GrideItme"
                    android:layout_width="50dp"
                    android:layout_height="25dp"
                    android:layout_marginLeft="1dp"
                    android:text="0" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="251dp"
                android:layout_height="26dp"

                android:orientation="horizontal">

                <TextView
                    style="@style/GrideItme"
                    android:layout_width="200dp"
                    android:layout_height="25dp"
                    android:text="50-70um异物检出数(颗)" />

                <TextView
                    android:id="@+id/tvCheckNum50"

                    style="@style/GrideItme"
                    android:layout_width="50dp"
                    android:layout_height="25dp"
                    android:layout_marginLeft="1dp"
                    android:text="0" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="251dp"
                android:layout_height="26dp"

                android:orientation="horizontal">

                <TextView
                    style="@style/GrideItme"
                    android:layout_width="200dp"
                    android:layout_height="25dp"
                    android:text="70um以上异物检出数(颗)X" />

                <TextView
                    android:id="@+id/tvCheckNum70"

                    style="@style/GrideItme"
                    android:layout_width="50dp"
                    android:layout_height="25dp"
                    android:layout_marginLeft="1dp"
                    android:text="0" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="51dp"
            android:layout_height="270dp"
            android:layout_marginLeft="1dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tvPiaoFuRes"
                style="@style/GrideItme"
                android:layout_width="50dp"
                android:layout_height="26dp"
                android:text="阴性" />

            <TextView
                android:id="@+id/tvSuJianRes"

                style="@style/GrideItme"
                android:layout_width="50dp"
                android:layout_height="51dp"
                android:layout_marginTop="1dp"
                android:text="阴性" />

            <TextView
                android:id="@+id/tvCheckRes"

                style="@style/GrideItme"
                android:layout_width="50dp"
                android:layout_height="103dp"
                android:layout_marginTop="1dp"
                android:text="阴性" />
            <TextView
                android:id="@+id/tvMinNumRes"

                style="@style/GrideItme"
                android:layout_width="50dp"
                android:layout_height="51dp"
                android:layout_marginTop="1dp"
                android:text="阴性" />
            <TextView
                android:id="@+id/tvMaxCheckRes"

                style="@style/GrideItme"
                android:layout_width="50dp"
                android:layout_height="25dp"
                android:layout_marginTop="1dp"
                android:text="阴性" />
        </LinearLayout>
    </LinearLayout>






</RelativeLayout>